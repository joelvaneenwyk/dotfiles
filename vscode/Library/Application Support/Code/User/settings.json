{
    "files.trimTrailingWhitespace": true,
    "scssLint.runOnTextChange": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.automationShell.osx": "/bin/bash",
    "git.autofetch": true,
    "ruby.useBundler": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.tabSize": 2,
    "workbench.colorTheme": "Monokai",
    "diffEditor.renderSideBySide": false,
    "dart.debugExternalLibraries": true,
    "dart.debugSdkLibraries": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.fontFamily": "JetBrains Mono, Hack, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 11,
    "terminal.integrated.fontSize": 11,
    "workbench.startupEditor": "none",
    "terminal.integrated.tabs.enabled": false,
    "cmake.configureOnOpen": true,
    "terminal.integrated.profiles.osx": {
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        }
    },
    "terminal.integrated.defaultProfile.osx": "fish",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "terminal.integrated.enablePersistentSessions": false,
    "dart.debugExternalPackageLibraries": true,
    "dart.flutterSdkPath": "/Users/jdve/.asdf/installs/flutter/2.10.1-stable",
    "liveshare.allowGuestDebugControl": true,
    "liveshare.allowGuestTaskControl": true,
    "liquid.format": false,
    "eslint.workingDirectories": [
        {
            "mode": "auto"
        }
    ],
    "eslint.format.enable": true,
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    }
}
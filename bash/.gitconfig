# This is Git's per-user configuration file.
[push]
	default = simple
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	pr = pull --rebase
	df = diff
[pull]
	rebase = true
[commit]
	verbose = true
	gpgsign = true
[core]
	excludesfile = /Users/jdve/.gitignore_global
	symlinks = true
[user]
	name = Jonathan Van Eenwyk
	email = jonathan@vihanti.com
	signingkey = 9FCE213AF5305953
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

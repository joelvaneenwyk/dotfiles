{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [{
            "alignment": "left",
            "newline": true,
            "segments": [{
                    "foreground": "#a3a3a3",
                    "style": "plain",
                    "template": "\u250c",
                    "type": "text"
                },
                {
                    "foreground": "#a3a3a3",
                    "style": "powerline",
                    "template": " MY\u2208 ",
                    "type": "text"
                },
                {
                    "background": "#0077c2",
                    "foreground": "#e4e4e4",
                    "properties": {
                        "display_distro_name": true,
                        "macos": "mac"
                    },
                    "style": "plain",
                    "template": " {{.Icon}}  ",
                    "type": "os"
                },
                {
                    "background": "#0077c2",
                    "foreground": "#e4e4e4",
                    "invert_powerline": true,
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "mapped_shell_names": {
                            "pwsh": "PS"
                        }
                    },
                    "style": "powerline",
                    "template": " {{ .Name }} ",
                    "type": "shell"
                },
                {
                    "foreground": "#757575",
                    "properties": {
                        "display_host": true
                    },
                    "style": "plain",
                    "template": " {{ if .SSHSession }}[SSH] {{ end }}{{ .UserName }}@{{ .HostName }} ",
                    "type": "session"
                },
                {
                    "background": "#61AFEF",
                    "foreground": "#474747",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "mixed_threshold": 1,
                        "style": "letter"
                    },
                    "style": "powerline",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "type": "exit",
                    "style": "powerline",
                    "foreground": "#B8BEC2",
                    "background": "#ffffff",
                    "trailing_diamond": "\ue0b4",
                    "template": "{{ if ne .Code 0 }}<,#FF461B> \uf656 {{ .Meaning }} </>{{else}}<,#8AEB07> \uf42e  </>{{ end }}",
                    "properties": {
                        "always_enabled": true
                    }
                }
            ],
            "type": "prompt"
        }, {
            "alignment": "right",
            "segments": [{
                    "foreground": "#ff8080",
                    "properties": {
                        "root_icon": "root"
                    },
                    "style": "plain",
                    "template": " <#757575,>as</> \uf0e7",
                    "type": "root"
                },
                {
                    "foreground": "#689f38",
                    "properties": {
                        "time_format": "15:04:05"
                    },
                    "style": "plain",
                    "template": " <#757575,>|</> {{ .CurrentDate | date .Format }}",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [{
                    "foreground": "#a3a3a3",
                    "style": "plain",
                    "template": "\u2514",
                    "type": "text"
                },
                {
                    "foreground": "#ffffff",
                    "style": "plain",
                    "template": "$ ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 2
}
